{
  "name": "migration-v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "(cd ./api && npm install --force) ; (cd ./ui && npm install --force) ; (cd ./upload-api && npm install --force)",
    "env": "npm start",
    "api": "cd ./api && npm run dev",
    "upload": "cd ./upload-api && npm start",
    "ui": "cd ./ui && npm start",
    "start": "node index.js",
    "build": "esbuild cli/packages/contentstack/ --bundle --platform=node --outfile=dist/cli.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentstack-expert-services/migration-v2-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/contentstack-expert-services/migration-v2-ui/issues"
  },
  "homepage": "https://github.com/contentstack-expert-services/migration-v2-ui#readme",
  "devDependencies": {
    "esbuild": "^0.24.0",
    "husky": "^4.3.8",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "husky": {
    "hooks": {}
  },
  "validate-branch-name": {
    "pattern": "^(feature|bugfix|hotfix)/[a-z0-9-]{5,30}$",
    "errorMsg": "Please add valid branch name!"
  },
  "bin": {
    "migration-v2": "./dist/cli.bundle.js"
  },
  "dependencies": {
    "commander": "^12.1.0"
  }
}
