{
  "name": "migration-v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "preinstall": "pnpm store prune && pnpm store clean && (command -v pnpm >/dev/null 2>&1 || npm install -g pnpm)",
    "postinstall": "npm-run-all install:api install:ui install:upload-api && npm run install:cli",
    "install:api": "cd ./api && npm install --force",
    "install:ui": "cd ./ui && npm install --force",
    "install:upload-api": "cd ./upload-api && npm install --force",
    "install:cli": "cd ./cli && npm run cache:clear && npm run clean && npm run setup-repo",
    "env": "npm start",
    "api": "cd ./api && pnpm run dev",
    "upload": "cd ./upload-api && pnpm start",
    "ui": "cd ./ui && pnpm start",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentstack-expert-services/migration-v2-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/contentstack-expert-services/migration-v2-ui/issues"
  },
  "homepage": "https://github.com/contentstack-expert-services/migration-v2-ui#readme",
  "devDependencies": {
    "husky": "^4.3.8",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "validate-branch-name": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^9.0.0"
  },
  "husky": {
    "hooks": {}
  },
  "validate-branch-name": {
    "pattern": "^(feature|bugfix|hotfix)/[a-z0-9-]{5,30}$",
    "errorMsg": "Please add valid branch name!"
  }
}
